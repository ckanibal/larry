{
  "name": "larry.node",
  "version": "0.3.0",
  "author": "kanibal <frustrum@pictor.uberspace.de>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "nodemon -r \"dotenv/config\" dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "mocha --exit -r \"dotenv/config\" -r ts-node/register src/**/*.spec.ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon -r \"dotenv/config\" --inspect --trace-warnings dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "migrate": "node_modules\\.bin\\migrate up --compiler \"ts:@babel/register\""
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/node": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-stage-3": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.15.3",
    "express-acl": "^1.0.3",
    "express-jwt": "^5.3.0",
    "express-param": "^1.0.2",
    "express-validator": "^4.2.0",
    "gridfs-stream": "^1.1.1",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "migrate": "^1.3.0",
    "mongoose": "^5.0.1",
    "mongoose-delete": "^0.4.0",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "rimraf": "^2.6.1",
    "sinon": "^4.0.1",
    "slug": "^0.9.1",
    "xmlbuilder": "^9.0.7"
  },
  "devDependencies": {
    "@types/acl": "^0.4.33",
    "@types/async": "^2.0.40",
    "@types/body-parser": "^1.16.4",
    "@types/chai": "^4.0.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.1",
    "@types/errorhandler": "^0.0.30",
    "@types/express": "^4.0.36",
    "@types/express-jwt": "^0.0.38",
    "@types/faker": "^4.1.2",
    "@types/gridfs-stream": "^0.5.29",
    "@types/http-status": "^0.2.29",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.43",
    "@types/mongoose": "^4.7.19",
    "@types/mongoose-paginate": "^5.0.5",
    "@types/morgan": "^1.7.32",
    "@types/multer-gridfs-storage": "^2.0.1",
    "@types/node": "^8.0.20",
    "@types/passport": "0.3.5",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.30",
    "@types/passport-strategy": "0.2.30",
    "@types/pluralize": "^0.0.27",
    "@types/slug": "^0.9.0",
    "@types/validator": "^6.2.3",
    "@types/xmlbuilder": "^0.0.31",
    "babel-jest": "^20.0.3",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-watch": "^3.1.2",
    "faker": "^4.1.0",
    "jest": "^20.0.4",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typegoose": "^3.8.0",
    "typescript": "^2.8.1"
  },
  "peerDependencies": {
    "@types/passport": "0.3.5"
  },
  "eslintConfig": {
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ]
  }
}
